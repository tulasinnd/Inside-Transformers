    "def vector_norm(x):\n",
    "    return x.norm(p=2).item()\n",
    "\n",
    "# function to extract the required values from the states\n",
    "def inspect_token_representation(states, token_index, layer_index):\n",
    "    \"\"\"\n",
    "    Inspect representation behavior for one token at one decoder layer.\n",
    "    Returns raw vectors + scalar stats (norms).\n",
    "    \"\"\"\n",
    "\n",
    "    layer_key = f\"layer_{layer_index}\"\n",
    "\n",
    "    # ---- extract vectors ----\n",
    "    x_embed = states[\"x_embed\"][0, token_index]\n",
    "\n",
    "    attn_out = states[layer_key][\"mha_x\"][0, token_index]\n",
    "    res1     = states[layer_key][\"res1\"][0, token_index]\n",
    "    ln1      = states[layer_key][\"ln1_x\"][0, token_index]\n",
    "\n",
    "    ffn_out  = states[layer_key][\"ffn_x\"][0, token_index]\n",
    "    res2     = states[layer_key][\"res2\"][0, token_index]\n",
    "    ln2      = states[layer_key][\"ln2_x\"][0, token_index]\n",
    "\n",
    "    # ---- compute norms ----\n",
    "    norms = {\n",
    "        \"embedding\"        : vector_norm(x_embed),\n",
    "        \"attention\"  : vector_norm(attn_out),\n",
    "        \"add_residual_1\" : vector_norm(res1),\n",
    "        \"layernorm_1\"        : vector_norm(ln1),\n",
    "        \"ffn\"        : vector_norm(ffn_out),\n",
    "        \"add_residual_2\" : vector_norm(res2),\n",
    "        \"layernorm_2\"        : vector_norm(ln2),\n",
    "    }\n",
    "\n",
    "    return norms\n"
   ]
