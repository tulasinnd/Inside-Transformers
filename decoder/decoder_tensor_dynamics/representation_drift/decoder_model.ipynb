   "source": [
    "## **Decoder Only Model: next-token prediction**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "2402f883",
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
