   "source": [
    "num_steps = 2000        # training iterations\n",
    "print_every = 200       # logging frequency\n",
    "\n",
    "model.train()\n",
    "\n",
    "total_loss = 0.0\n",
    "\n",
    "for step in range(1, num_steps + 1):\n",
    "\n",
    "    # ---- Get batch ----\n",
    "    X, Y = get_batch(tokenized_train_text, batch_size, seq_len, device=device)\n",
    "\n",
    "    # ---- Forward ----\n",
    "    logits = model(X)          # (B, S, V)\n",
    "\n",
    "    B, S, V = logits.shape\n",
    "    logits = logits.view(B * S, V)\n",
    "    Y = Y.view(B * S)\n",
    "\n",
    "    # ---- Loss ----\n",
    "    loss = loss_CE(logits, Y)\n",
    "\n",
    "    # ---- Backward ----\n",
    "    optimizer.zero_grad()\n",
    "    loss.backward()\n",
    "    optimizer.step()\n",
    "\n",
    "    total_loss += loss.item()\n",
    "\n",
    "    # ---- Logging ----\n",
    "    if step % print_every == 0:\n",
    "        avg_loss = total_loss / print_every\n",
    "        print(f\"Step {step} | Avg Loss: {avg_loss:.4f}\")\n",
    "        total_loss = 0.0\n"
   ]
