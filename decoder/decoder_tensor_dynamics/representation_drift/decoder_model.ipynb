    "## **Cosine similarity:**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "07d86b29",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "cos(embedding → layer0) = 0.6749\n",
      "cos(layer0 → layer1)    = 0.8023\n",
      "cos(embedding → layer1) = 0.4570\n"
     ]
    }
   ],
   "source": [
    "import torch\n",
    "\n",
    "def cosine_sim(x, y, eps=1e-8):\n",
    "    \"\"\"\n",
    "    x, y: 1D tensors (d_model,)\n",
    "    \"\"\"\n",
    "    return torch.dot(x, y) / (x.norm() * y.norm() + eps)\n",
    "\n",
    "token_index =  4 # \"mouse\"\n",
    "x_embed = states[\"x_embed\"][0, token_index]\n",
    "x_layer_0 = states[\"layer_0\"][\"ln2_x\"][0, token_index]\n",
    "x_layer_1 = states[\"layer_1\"][\"ln2_x\"][0, token_index]\n",
    "\n",
    "cos_emb_l0 = cosine_sim(x_embed, x_layer_0)\n",
    "cos_l0_l1  = cosine_sim(x_layer_0, x_layer_1)\n",
    "cos_emb_l1 = cosine_sim(x_embed, x_layer_1)\n",
    "\n",
    "print(f\"cos(embedding → layer0) = {cos_emb_l0:.4f}\")\n",
    "print(f\"cos(layer0 → layer1)    = {cos_l0_l1:.4f}\")\n",
    "print(f\"cos(embedding → layer1) = {cos_emb_l1:.4f}\")"
   ]
  },
